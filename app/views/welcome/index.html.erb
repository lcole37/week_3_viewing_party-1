<% if session[:user_id] %>
  <%= link_to "Logout", logout_path, method: :get %>
<% else %>
  <%= button_to "Create New User", register_path, method: :get %>
<% end %>

<section class='existing-users'>
<% if session[:user_id] %>

      <p>Existing Users: </p>
      <% @users.each do |user| %>
          <li> <%= user.email %></li>
      <% end %>


<% end %>
</section>
